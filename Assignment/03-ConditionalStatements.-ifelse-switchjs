

console.log("*****executing browsername funtion *******");

function launchBrowser(browsername){
   
        if(browsername == "chrome"){
            console.log("browsername is chrome");
        }
        else if(browsername == "firefox"){
            console.log("browsername is firefox");
        }
        else if(browsername == "edge"){
            console.log("browsername is edge");
        }
        else {
            console.log("unsupported browsername entered");
        }
}

let browsername = "edge";
launchBrowser(browsername);

console.log("*****executing switch case funtion *******");

function runTests(testname){

    switch(testname){
        case 'smoke':
            console.log('smoke test');
            break;
        case 'sanity':
            console.log('sanity test');
            break;
        case 'regression':
            console.log('regression test');
            break;
        default:
            console.log("default selected-");
            break;
}
}
let testname = "sanity";
runTests(testname);